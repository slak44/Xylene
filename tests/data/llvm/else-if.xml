<!--
if false do
  return 1;
else if true do
  return 2;
else 
  return 3;
end
-->
<block type="root">
  <branch>
    <expr type="Boolean" value="false"/>
    <block type="if">
      <return>
        <expr type="Integer" value="1"/>
      </return>
    </block>
    <branch>
      <expr type="Boolean" value="true"/>
      <block type="if">
        <return>
          <expr type="Integer" value="2"/>
        </return>
      </block>
      <block type="if">
        <return>
          <expr type="Integer" value="3"/>
        </return>
      </block>
    </branch>
  </branch>
</block>
